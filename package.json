{
  "name": "matrix-backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx tsc -w & nodemon dist/index.js",
    "build": "rm -rf dist && npx tsc",
    "start": "node dist/index.js",
    "migrate": "npx ts-node ./node_modules/.bin/knex migrate:latest",
    "rollback": "npx ts-node ./node_modules/.bin/knex migrate:rollback",
    "seeds": "npx ts-node ./node_modules/.bin/knex seed:run",
    "test": "jest --runInBand --verbose --env=node",
    "test:watch": "jest --watch --env=node",
    "test:coverage": "jest --coverage --env=node"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "~4.16.1",
    "generate-unique-id": "^2.0.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/knex": "^0.15.2",
    "@types/node": "^22.10.2",
    "@types/nodemon": "^1.19.6",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
